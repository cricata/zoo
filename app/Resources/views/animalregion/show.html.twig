{% extends 'base.html.twig' %}

{% block body %}
    <h1>AnimalRegion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ animalRegion.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ animalRegion.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ animalRegion.description }}</td>
            </tr>
            <tr>
                <th>Datcre</th>
                <td>{% if animalRegion.datCre %}{{ animalRegion.datCre|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Datupd</th>
                <td>{% if animalRegion.datUpd %}{{ animalRegion.datUpd|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('animalregion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('animalregion_edit', { 'id': animalRegion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
